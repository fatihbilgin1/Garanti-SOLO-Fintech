{
  "openapi": "3.0.1",
  "info": {
    "title": "GarantiWebApi",
    "version": "1.0"
  },
  "paths": {
    "/api/Dashboard/summary": {
      "get": {
        "tags": [
          "Dashboard"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Dashboard/transactions": {
      "get": {
        "tags": [
          "Dashboard"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Link/create": {
      "post": {
        "tags": [
          "Link"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentLink"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentLink"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PaymentLink"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Link/list": {
      "get": {
        "tags": [
          "Link"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PaymentLink": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "clientName": {
            "type": "string",
            "nullable": true
          },
          "projectDescription": {
            "type": "string",
            "nullable": true
          },
          "amount": {
            "type": "number",
            "format": "double"
          },
          "status": {
            "type": "boolean"
          },
          "createdDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      }
    }
  }
}